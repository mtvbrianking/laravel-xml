{
  "name": "bmatovu/laravel-xml",
  "description": "Laravel XML Support",
  "homepage": "https://github.com/mtvbrianking/laravel-xml",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "package",
    "xml",
    "middleware",
    "response",
    "support"
  ],
  "authors": [
    {
      "name": "Brian Matovu",
      "email": "mtvbrianking@gmail.com",
      "homepage": "http://bmatovu.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "illuminate/http": "~5.3.0|~5.4.0|~5.5.0|~5.6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0|~5.7|~6.0|^7.0",
    "orchestra/testbench": "~3.3.0|~3.4.0|~3.5.0|~3.6.0"
  },
  "autoload": {
    "psr-4": {
      "Bmatovu\\LaravelXml\\": "src/"
    },
    "files": [
      "src/Support/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Bmatovu\\LaravelXml\\Test\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "extra": {
    "laravel": {
      "providers": [
        "Bmatovu\\LaravelXml\\LaravelXmlServiceProvider"
      ],
      "aliases": {
        "Xml": "Bmatovu\\LaravelXml\\LaravelXml"
      }
    }
  }
}
